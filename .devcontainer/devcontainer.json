{
    "name": "smart-alpha-web",
    "dockerComposeFile": ["../docker-compose.yml"],
    "service": "web",
    "workspaceFolder": "/app",
    "settings": {
      "editor.tabSize": 2,
      "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
      }
    },
    "extensions": [
      "octref.vetur",
      "dbaeumer.vscode-eslint"
    ],
    "forwardPorts": [3000]
}
